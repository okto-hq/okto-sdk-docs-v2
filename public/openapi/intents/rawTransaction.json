        "description": "Okto App Secret",
        "example": "YOUR_OKTO_APP_SECRET"
      },
      "oauth2": {
        "type": "oauth2",
        "flows": {
          "implicit": {
            "authorizationUrl": "https://galaxy.scalar.com/oauth/authorize",
